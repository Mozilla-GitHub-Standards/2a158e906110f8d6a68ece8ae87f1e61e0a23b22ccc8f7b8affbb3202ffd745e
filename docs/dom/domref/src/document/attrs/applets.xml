<refentry id="applets">
  <refmeta>
    <refentrytitle>applets</refentrytitle>
  </refmeta>

  <refnamediv>
    <refname>applets</refname>
    <refpurpose>returns an ordered list of the applets within a document.</refpurpose>
  </refnamediv>

  <refsynopsisdiv>
  <funcsynopsis>
   <funcprototype>
    <funcdef>app_list = document.applets</funcdef>
    <paramdef>app_list is a nodeList of the applets within the document.</paramdef>
   </funcprototype>
  </funcsynopsis>
  </refsynopsisdiv>

  <refsect1><title>Notes</title>
    <para>None.</para>
  </refsect1>
  
  <refsect1><title>Specification</title>
    <ulink url="http://www.w3.org/TR/2000/WD-DOM-Level-2-HTML-20001113/html.html#ID-85113862">applets</ulink>
  </refsect1>

</refentry>
